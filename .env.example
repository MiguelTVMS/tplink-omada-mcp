# Copy this file to `.env` and fill in the values before running the server

# Required settings
OMADA_BASE_URL=https://omada-controller.example.com
OMADA_USERNAME=admin
OMADA_PASSWORD=changeme

# Optional settings
# Default site identifier used when one is not provided for commands
OMADA_SITE_ID=
# Set to "false" to disable strict TLS validation (defaults to true)
OMADA_STRICT_SSL=true
# Timeout for controller requests in milliseconds (e.g. 30000)
OMADA_TIMEOUT=
# HTTPS proxy URL (e.g. http://proxy:3128)
OMADA_PROXY_URL=
